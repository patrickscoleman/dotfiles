# Directory Shortcuts
alias ll="exa -lh"
alias la="exa -lah"
alias t="touch"

# Applications
alias finder="open ."
alias chrome='open -a "Google Chrome"'

# Editing Shortcuts
alias zed="vim ~/.zshrc"
alias alied="vim ~/.bash_aliases"

# Git
alias gs="git status"
alias gan="git add --intent-to-add . && git add --patch"

# nvm
alias nn="nvm use default"
